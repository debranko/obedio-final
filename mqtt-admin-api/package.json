{
  "name": "obedio-mqtt-admin-api",
  "version": "1.0.0",
  "description": "MQTT administration API service for Obedio IoT device management",
  "main": "dist/server.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc",
    "start": "node bootstrap.js",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "docker:build": "docker build -t obedio-mqtt-api .",
    "docker:run": "docker run -p 3001:3001 obedio-mqtt-api"
  },
  "keywords": [
    "mqtt",
    "iot",
    "device-management",
    "websocket",
    "realtime"
  ],
  "author": "Obedio Team",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^5.10.2",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.3",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "joi": "^17.12.2",
    "jsonwebtoken": "^9.0.2",
    "module-alias": "^2.2.3",
    "mqtt": "^5.3.5",
    "pino": "^8.19.0",
    "pino-http": "^9.0.0",
    "pino-pretty": "^10.3.1",
    "prisma": "^5.10.2",
    "qrcode": "^1.5.3",
    "tsconfig-paths": "^4.2.0",
    "uuid": "^9.0.1",
    "ws": "^8.16.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.11.20",
    "@types/qrcode": "^1.5.5",
    "@types/uuid": "^9.0.8",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "@vitest/coverage-v8": "^1.3.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "prettier": "^3.2.5",
    "tsx": "^4.7.1",
    "typescript": "^5.3.3",
    "vitest": "^1.3.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
